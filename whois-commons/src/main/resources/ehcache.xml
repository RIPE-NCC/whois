<?xml version="1.0" encoding="utf-8"?>
<config
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xmlns='http://www.ehcache.org/v3'
    xsi:schemaLocation="http://www.ehcache.org/v3 http://www.ehcache.org/schema/ehcache-core.xsd">

    <cache alias="ssoValidateToken">
        <key-type>java.lang.String</key-type>
        <value-type>net.ripe.db.whois.common.sso.domain.ValidateTokenResponse</value-type>
        <expiry>
            <ttl>60</ttl>
        </expiry>
        <resources>
            <heap>10000</heap>
        </resources>
    </cache>

    <cache alias="ssoUuid">
        <key-type>java.lang.String</key-type>
        <value-type>java.lang.String</value-type>
        <expiry>
            <ttl>60</ttl>
        </expiry>
        <resources>
            <heap>10000</heap>
        </resources>
    </cache>

    <cache alias="ssoUserDetails">
        <key-type>java.lang.String</key-type>
        <value-type>net.ripe.db.whois.common.sso.domain.ValidateTokenResponse</value-type>
        <expiry>
            <ttl>60</ttl>
        </expiry>
        <resources>
            <heap>10000</heap>
        </resources>
    </cache>

</config>
